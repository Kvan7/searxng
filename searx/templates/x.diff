<                     <th scope="col" class="failed-test">{{ _('Failed test') }}</th>
<                     <th scope="col">{{ _('Comment(s)') }}</th>
<                 </tr>
<                 {% for test_name, results in engine_reliabilities[selected_engine_name].checker.items() %}
