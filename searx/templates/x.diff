<                     <td>
<                         {% for r in results %}<p>{{ r }}</p>{% endfor %}
<                     </td>
<                 </tr>
<                 {% endfor %}
<             </table>
<         {% endif %}
<         {{ new_issue(selected_engine_name, engine_reliabilities[selected_engine_name]) }}
